<% if !wishes.empty? %>
    <div>
        <%= local_assigns[:wishescat].to_s %>
    </div>
    <div>
        <% wishes.each do |wish| %>
            <div style="width:100%">
                <div style="width: 200px; display: inline-block;">
                    <%= wish.type.typeName %> :
                </div>              
                <div  style="width: 200px; display: inline-block;">
                    <label>Importance</label>
                    <%= Propertyadwish.levels.key(wish.wishlevel) %>
                </div>
                <div  style="width: 600px; display: inline-block;">
                    <label>Commentaire</label>
                    <%= wish.comment %>
                </div>
            </div>
        <% end %>
    </div>
<% end %>